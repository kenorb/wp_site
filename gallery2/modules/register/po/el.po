msgid "Register"
msgstr "Εγγραφείτε"

msgid "New user registration:"
msgstr "Εγγραφή Νέου Χρήστη:"

#, c-format
msgid "    Username: %s"
msgstr "    Όνομα Χρήστη: %s"

#, c-format
