msgstr "نسخه‌ی کتابخانه‌ی GD"

msgid "GD version"
msgstr "نسخه‌ی GD"

msgid "Required"
msgstr "لازم"

msgid "Pass/fail"
msgstr "گذر\\شکست"

msgid "bundled"
msgstr "بسته"

msgid "Failed"
msgstr "ناموفق"

msgid ""
"This GD version is too old and is not supported by this module! Please upgrade your PHP "
"installation to include the latest GD version."
msgstr ""

