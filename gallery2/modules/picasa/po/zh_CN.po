msgid "Importing albums"
msgstr "导入相册"

msgid "Importing photos"
msgstr "导入照片"

#, c-format
msgid "%d megabyte"
msgid_plural "%d megabytes"
msgstr[0] "%d MB"

#, c-format
msgid "%d kilobytes"
msgid_plural "%d kilobytes"
msgstr[0] "%d KB"

msgid "Base filename"
msgstr "基础文件名"

