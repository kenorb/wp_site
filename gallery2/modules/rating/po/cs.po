msgid_plural "%d votes"
msgstr[0] "%d hlas"
msgstr[1] "%d hlasy"
msgstr[2] "%d hlasů"

msgid "&laquo; default theme &raquo;"
msgstr "&laquo; výchozí téma &raquo;"

