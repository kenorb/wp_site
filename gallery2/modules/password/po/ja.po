msgid "Password Entry"
msgstr "パスワードエントリ"

msgid "Password Protected Albums"
msgstr "パスワードで保護されたアルバム"

msgid "Assign passwords for viewing particular albums"
msgstr "特別なアルバムを見るためのパスワードを割り当てます"

msgid "Display"
msgstr "表示"

msgid "Password Items"
msgstr "パスワードアイテム"

msgid "Set Password"
msgstr "パスワードを設定"

msgid "Update permissions"
msgstr "パーミッションを更新"

msgid "Remove Password"
msgstr "パスワードを削除"

